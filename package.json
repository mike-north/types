{
  "name": "@mike-north/types",
  "version": "1.2.2",
  "description": "TypeScript types/interfaces I often use for small libraries",
  "main": "dist/commonjs/src/index.js",
  "module": "dist/modules/src/index.js",
  "typings": "dist/types/types.d.ts",
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "preversion": "yarn test",
    "clean": "scripty",
    "lint": "yarn eslint -c ./.eslintrc.js --ext .ts src test type-tests",
    "pretest": "scripty",
    "test": "scripty",
    "build": "SCRIPTY_PARALLEL=true scripty",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-documenter": "7.7.12",
    "@microsoft/api-extractor": "7.7.8",
    "@types/qunit": "2.9.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "dtslint": "1.0.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "prettier": "1.19.1",
    "qunit": "2.9.3",
    "qunit-decorators": "1.1.5",
    "rimraf": "3.0.1",
    "scripty": "1.9.1",
    "travis-deploy-once": "5.0.11",
    "typescript": "3.7.5"
  },
  "optionalDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@mike-north/js-lib-renovate-config": "1.3.1",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.1",
    "@semantic-release/npm": "^7.0.2",
    "husky": "^3.0.0",
    "semantic-release": "^17.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-north/types.git"
  },
  "plublishConfig": {
    "access": "public",
    "tag": "next"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  },
  "engines": {
    "node": "8.11 - 8.17 || 10.* || >= 12"
  },
  "volta": {
    "node": "10.18.1",
    "yarn": "1.21.1"
  }
}
